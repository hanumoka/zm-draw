{
  "name": "@zm-draw/core",
  "version": "0.1.0",
  "description": "Core types, constants, and utilities for zm-draw diagram editor",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
