    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsdown": "^0.20.3",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "diagram",
    "types",
    "canvas",
    "whiteboard"
  ]
